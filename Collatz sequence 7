#include <stdio.h>

int main() {
    int n, count = 0;
    scanf("%d", &n);
    printf("Enter a number\n");

    while (n != 1) {
        printf("%d\n", n);
        if (n % 2 == 0)
            n = n / 2;
        else
            n = 3 * n + 1;
        count++;
    }
    printf("1\n");
    printf("count:%d\n", count);

    return 0;
}
