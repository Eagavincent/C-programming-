#include <stdio.h>

int main() {
    char name[100];
    int arrear;
    float cgpa;
    scanf("%s", name);
    scanf("%d", &arrear);
    scanf("%f", &cgpa);
    printf("Name of the Student:%s\n", name);
    if ((arrear == 1 && cgpa > 70) || ((arrear == 1 || arrear == 2) && cgpa > 75)) {
        printf("%s is Eligible for Placement\n", name);
    } else {
        printf("%s is Not Eligible for Placement\n", name);
    }

    return 0;
}
